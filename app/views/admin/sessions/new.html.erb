<h2>Log in</h2>

<%= form_with model: @admin, url: admin_session_path do |f| %>
  <div class="field">
    <%= f.label :email,"メールアドレス" %><br />
    <%= f.text_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password,"パスワード" %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% end %>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "admin/shared/links" %>
